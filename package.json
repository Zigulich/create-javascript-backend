	"license": "MIT",
	"private": true,
	"dependencies": {
		"cors": "^2.8.5",
		"date-fns": "^2.29.3",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.0",
		"@babel/node": "^7.20.7",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.1",
